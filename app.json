{
  "expo": {
    "name": "AteneApp",
    "slug": "AteneApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/img/Logo98.png",
    "splash": {
      "image": "./src/img/Logotipo-ATENEA.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffcc67"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./src/img/Logo98.png"
    },
    "android": {
      "package": "com.ivan95mq.AteneApp",
      "googleServicesFile": "./google-services.json",
      "useNextNotificationsApi": true
    },
    "description": "Aplicacion para el colegio atenea de merida",
    "githubUrl": "https://github.com/Ivii95/ColegioAtenea"
  }
}
